URxvt.perl-ext-common: dafault,clipboard,resize-font

URxvt.clipboard.autocopy: true
URxvt.keysim.M-c: perl:clipboard:copy
URxvt.keysim.M-v: perl:clipboard:paste
URxvt.resize-font.smaller: C-j
URxvt.resize-font.bigger: C-k
URxvt.iso14755: false
